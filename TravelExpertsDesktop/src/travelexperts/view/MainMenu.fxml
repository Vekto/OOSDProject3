<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="85.0" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="travelexperts.controller.MainMenuController">
   <children>
      <TilePane alignment="CENTER" prefHeight="400.0" prefRows="1" prefTileHeight="100.0" prefTileWidth="100.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button focusTraversable="false" minWidth="91.0" mnemonicParsing="false" prefHeight="91.0" prefWidth="91.0" text="Products">
               <font>
                  <Font name="Arial Narrow Bold" size="15.0" />
               </font>
               <effect>
                  <Reflection>
                     <input>
                        <DropShadow />
                     </input>
                  </Reflection>
               </effect>
            </Button>
            <Button fx:id="btnCustomers" focusTraversable="false" minWidth="91.0" mnemonicParsing="false" onAction="#onCustomersClick" prefHeight="91.0" prefWidth="91.0" text="Customers">
               <effect>
                  <Reflection>
                     <input>
                        <DropShadow />
                     </input>
                  </Reflection>
               </effect>
            </Button>
            <Button focusTraversable="false" minWidth="91.0" mnemonicParsing="false" prefHeight="91.0" prefWidth="91.0" text="Suppliers">
               <effect>
                  <Reflection>
                     <input>
                        <DropShadow />
                     </input></Reflection>
               </effect>
            </Button>
            <Button focusTraversable="false" minWidth="91.0" mnemonicParsing="false" onAction="#onAgentsClick" prefHeight="91.0" prefWidth="91.0" text="Agents">
               <effect>
                  <Reflection>
                     <input>
                        <DropShadow />
                     </input>
                  </Reflection>
               </effect></Button>
         </children>
      </TilePane>
   </children>
</AnchorPane>
